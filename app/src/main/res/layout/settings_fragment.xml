<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/swipeRefreshLayout"
    android:layout_height="match_parent"
    android:background="@color/light_grey_background">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fillViewport="true">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_10sdp"
                android:background="@drawable/curve_white_selector"
                android:elevation="@dimen/_4sdp"
                android:onClick="onClick"
                android:padding="@dimen/_10sdp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:targetApi="lollipop">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/dukaanNameTextView"
                    style="@style/common_bold_text_view"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/app_name"
                    android:textSize="@dimen/_12sdp"
                    app:layout_constraintBottom_toBottomOf="@+id/linearLayout"
                    app:layout_constraintEnd_toStartOf="@+id/linearLayout"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/linearLayout" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/profileStatusRecyclerView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintEnd_toEndOf="@+id/linearLayout"
                    app:layout_constraintStart_toStartOf="@+id/dukaanNameTextView"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout"
                    tools:itemCount="5"
                    tools:listitem="@layout/profile_status_item" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/completeProfileTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:layout_marginBottom="@dimen/_5sdp"
                    tools:text="@string/complete_profile"
                    android:textColor="@color/black"
                    android:fontFamily="@font/montserrat"
                    android:textSize="@dimen/_11sdp"
                    android:paddingEnd="@dimen/_5sdp"
                    android:paddingStart="@dimen/_5sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/shapeableImageView"
                    app:layout_constraintTop_toBottomOf="@id/profileStatusRecyclerView" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/stepsLeftTextView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:paddingStart="@dimen/_5sdp"
                    android:paddingEnd="@dimen/_5sdp"
                    android:fontFamily="@font/montserrat"
                    android:text="@string/complete_profile"
                    android:textColor="@color/default_text_light_grey"
                    android:textSize="@dimen/_11sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/completeProfileTextView"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/profileStatusRecyclerView" />

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/shapeableImageView"
                    android:layout_width="@dimen/_12sdp"
                    android:layout_height="@dimen/_12sdp"
                    android:background="@color/white"
                    android:src="@drawable/round_black_background"
                    app:layout_constraintBottom_toBottomOf="@+id/completeProfileTextView"
                    app:layout_constraintEnd_toEndOf="@+id/linearLayout"
                    app:layout_constraintTop_toTopOf="@+id/completeProfileTextView" />

                <LinearLayout
                    android:id="@+id/linearLayout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/curve_black_dotted_border"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingStart="@dimen/_5sdp"
                    android:paddingTop="@dimen/_10sdp"
                    android:paddingEnd="@dimen/_5sdp"
                    android:paddingBottom="@dimen/_10sdp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <com.google.android.material.imageview.ShapeableImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_photography" />

                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:maxLines="1"
                        android:textColor="@color/black"
                        android:fontFamily="@font/montserrat"
                        android:paddingTop="@dimen/_2sdp"
                        android:text="@string/add_logo"
                        android:textSize="@dimen/_9sdp" />

                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintLayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@color/white"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/materialTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_13sdp"
                    android:text="@string/store_controls"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_13sdp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/storeStatusTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:padding="@dimen/_2sdp"
                    android:textColor="@color/black"
                    app:layout_constraintEnd_toEndOf="@id/storeSwitch"
                    app:layout_constraintStart_toStartOf="@id/storeSwitch"
                    app:layout_constraintTop_toBottomOf="@id/storeSwitch"
                    tools:text="@string/store_controls" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/deliveryStatusTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/_2sdp"
                    android:textColor="@color/black"
                    app:layout_constraintBottom_toBottomOf="@+id/storeStatusTextView"
                    app:layout_constraintEnd_toEndOf="@+id/deliverySwitch"
                    app:layout_constraintStart_toStartOf="@+id/deliverySwitch"
                    app:layout_constraintTop_toTopOf="@+id/storeStatusTextView"
                    tools:text="@string/store_controls" />

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/moreControlsImageView"
                    android:layout_width="@dimen/_33sdp"
                    android:layout_height="0dp"
                    android:onClick="onClick"
                    android:background="?android:selectableItemBackground"
                    android:src="@drawable/ic_more_controls"
                    app:layout_constraintBottom_toTopOf="@+id/deliveryStatusTextView"
                    app:layout_constraintEnd_toEndOf="@+id/moreControlsTextView"
                    app:layout_constraintStart_toStartOf="@+id/moreControlsTextView"
                    app:layout_constraintTop_toTopOf="@+id/deliverySwitch" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/moreControlsTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/_2sdp"
                    android:text="@string/more_controls"
                    android:textColor="@color/black"
                    app:layout_constraintBottom_toBottomOf="@+id/deliveryStatusTextView"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.8"
                    app:layout_constraintStart_toEndOf="@+id/deliveryStatusTextView"
                    app:layout_constraintTop_toTopOf="@+id/deliveryStatusTextView" />

                <androidx.appcompat.widget.SwitchCompat
                    android:id="@+id/storeSwitch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:thumb="@drawable/ic_store_switch_selector"
                    app:layout_constraintStart_toStartOf="@+id/materialTextView"
                    app:layout_constraintTop_toBottomOf="@+id/materialTextView"
                    app:track="@drawable/ic_store_switch_track" />

                <androidx.appcompat.widget.SwitchCompat
                    android:id="@+id/deliverySwitch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:thumb="@drawable/ic_delivery_switch_selector"
                    app:layout_constraintStart_toEndOf="@+id/storeSwitch"
                    app:layout_constraintTop_toTopOf="@+id/storeSwitch"
                    app:track="@drawable/ic_store_switch_track" />

                <View
                    android:id="@+id/separator"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_13sdp"
                    android:background="@color/curve_grey_border"
                    app:layout_constraintTop_toBottomOf="@+id/deliveryStatusTextView" />

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/shareShowRoomWithCustomerTextView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_13sdp"
                    android:layout_marginBottom="@dimen/_13sdp"
                    android:text="@string/share_your_showroom_with_customers"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_13sdp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/whatsAppTextView"
                    app:layout_constraintStart_toStartOf="@+id/materialTextView"
                    app:layout_constraintTop_toBottomOf="@+id/separator" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/whatsAppTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="@+id/shareShowRoomWithCustomerTextView"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:background="@drawable/curve_black_border"
                    android:gravity="center"
                    android:onClick="onClick"
                    android:orientation="horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/shareShowRoomWithCustomerTextView">

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/shapeableImageView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_whatsapp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="1.0" />

                    <TextView
                        android:id="@+id/textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:clickable="false"
                        android:text="@string/share"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_14sdp"
                        app:layout_constraintBottom_toBottomOf="@+id/shapeableImageView2"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/shapeableImageView2"
                        app:layout_constraintTop_toTopOf="@+id/shapeableImageView2"
                        app:layout_constraintVertical_bias="0.3" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/newReleaseLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/newReleaseHeading"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_13sdp"
                    android:fontFamily="@font/montserrat"
                    android:text="@string/new_release"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_13sdp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <LinearLayout
                    android:id="@+id/viewTopOrderLayout"
                    android:layout_width="@dimen/_90sdp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/curve_white_selector"
                    android:elevation="@dimen/_5sdp"
                    android:onClick="onClick"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    app:layout_constraintBottom_toBottomOf="@+id/digitalShowroomWebLayout"
                    app:layout_constraintEnd_toStartOf="@+id/digitalShowroomWebLayout"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/digitalShowroomWebLayout"
                    tools:targetApi="lollipop">

                    <TextView
                        android:id="@+id/newTextView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/montserrat_bold"
                        android:gravity="center"
                        android:padding="@dimen/_4sdp"
                        tools:text="@string/text_new"
                        android:clickable="false"
                        android:textColor="@color/red"
                        android:textSize="@dimen/_8sdp" />

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/viewTopStoreImageView"
                        android:layout_width="wrap_content"
                        android:clickable="false"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/_5sdp"
                        android:src="@drawable/ic_auto_data_backup" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/viewTopStoreTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:gravity="center"
                        android:minLines="@integer/min_lines_two"
                        android:padding="@dimen/_2sdp"
                        android:clickable="false"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_10sdp"
                        app:fontFamily="@font/montserrat"
                        app:layout_constraintEnd_toEndOf="@+id/storeSwitch"
                        app:layout_constraintStart_toStartOf="@+id/storeSwitch"
                        app:layout_constraintTop_toBottomOf="@+id/storeSwitch"
                        tools:text="@string/view_top_store" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/digitalShowroomWebLayout"
                    android:layout_width="@dimen/_90sdp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:layout_marginBottom="@dimen/_15sdp"
                    android:background="@drawable/curve_white_selector"
                    android:elevation="@dimen/_5sdp"
                    android:onClick="onClick"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/bulkUploadItemLayout"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/viewTopOrderLayout"
                    app:layout_constraintTop_toBottomOf="@+id/newReleaseHeading"
                    tools:targetApi="lollipop">

                    <TextView
                        android:id="@+id/trendingTextView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:drawablePadding="@dimen/_5sdp"
                        android:fontFamily="@font/montserrat"
                        android:padding="@dimen/_4sdp"
                        android:clickable="false"
                        tools:text="@string/trending"
                        android:textColor="@color/red"
                        android:textSize="@dimen/_8sdp"
                        app:drawableStartCompat="@drawable/ic_trending" />

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/digitalShowroomWebImageView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/_5sdp"
                        android:clickable="false"
                        android:src="@drawable/ic_auto_data_backup" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/digitalShowroomWebTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:gravity="center"
                        android:maxLines="@integer/min_lines_two"
                        android:minLines="@integer/min_lines_two"
                        android:padding="@dimen/_2sdp"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_10sdp"
                        android:clickable="false"
                        app:fontFamily="@font/montserrat"
                        app:layout_constraintEnd_toEndOf="@+id/storeSwitch"
                        app:layout_constraintStart_toStartOf="@+id/storeSwitch"
                        app:layout_constraintTop_toBottomOf="@+id/storeSwitch"
                        tools:text="@string/digital_showroom_web" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/bulkUploadItemLayout"
                    android:layout_width="@dimen/_90sdp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/curve_white_selector"
                    android:elevation="@dimen/_5sdp"
                    android:onClick="onClick"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    app:layout_constraintBottom_toBottomOf="@+id/digitalShowroomWebLayout"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/digitalShowroomWebLayout"
                    app:layout_constraintTop_toTopOf="@+id/digitalShowroomWebLayout"
                    tools:targetApi="lollipop">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:drawablePadding="@dimen/_5sdp"
                        android:fontFamily="@font/montserrat"
                        android:padding="@dimen/_4sdp"
                        android:clickable="false"
                        android:textColor="@color/red"
                        android:textSize="@dimen/_8sdp" />

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/bulkUploadItemImageView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:clickable="false"
                        android:layout_margin="@dimen/_5sdp"
                        android:src="@drawable/ic_auto_data_backup" />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/bulkUploadItemTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:gravity="center"
                        android:clickable="false"
                        android:maxLines="@integer/min_lines_two"
                        android:minLines="@integer/min_lines_two"
                        android:padding="@dimen/_2sdp"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_10sdp"
                        app:fontFamily="@font/montserrat"
                        app:layout_constraintEnd_toEndOf="@+id/storeSwitch"
                        app:layout_constraintStart_toStartOf="@+id/storeSwitch"
                        app:layout_constraintTop_toBottomOf="@+id/storeSwitch"
                        tools:text="@string/bulk_upload_items" />

                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:padding="@dimen/_10sdp"
                android:orientation="vertical"
                app:layout_constraintTop_toBottomOf="@+id/newReleaseLayout">

                <LinearLayout
                    android:id="@+id/storeOptionOneLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:onClick="onClick"
                    android:gravity="center_vertical"
                    android:background="?android:selectableItemBackground"
                    android:weightSum="1"
                    android:orientation="horizontal">

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/storeOptionOneLeftImageView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.1"
                         />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/storeOptionOneTextView"
                        android:layout_width="0dp"
                        android:layout_weight="0.8"
                        android:clickable="false"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/montserrat"
                        android:padding="@dimen/_10sdp"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_13sdp"
                        tools:text="@string/refer_earn" />

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/storeOptionOneRightImageView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.1"
                        android:visibility="gone"
                        android:src="@drawable/ic_half_arrow_forward" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:background="@color/light_grey_background" />

                <LinearLayout
                    android:id="@+id/storeOptionTwoLayout"
                    android:background="?android:selectableItemBackground"
                    android:layout_width="match_parent"
                    android:onClick="onClick"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/storeOptionTwoLeftImageView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.1"
                         />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/storeOptionTwoTextView"
                        android:layout_width="0dp"
                        android:layout_weight="0.8"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/montserrat"
                        android:padding="@dimen/_10sdp"
                        android:clickable="false"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_13sdp"
                        tools:text="@string/refer_earn" />

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/storeOptionTwoRightImageView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.1"
                        android:visibility="gone"
                        android:src="@drawable/ic_half_arrow_forward" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:background="@color/light_grey_background" />

                <LinearLayout
                    android:id="@+id/storeOptionThreeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:onClick="onClick"
                    android:background="?android:selectableItemBackground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/storeOptionThreeLeftImageView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.1"
                         />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/storeOptionThreeTextView"
                        android:layout_width="0dp"
                        android:layout_weight="0.8"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/montserrat"
                        android:clickable="false"
                        android:padding="@dimen/_10sdp"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_13sdp"
                        tools:text="@string/refer_earn" />

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/storeOptionThreeRightImageView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.1"
                        android:visibility="gone"
                        android:src="@drawable/ic_half_arrow_forward" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:background="@color/light_grey_background" />

                <LinearLayout
                    android:id="@+id/storeOptionFourLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:selectableItemBackground"
                    android:onClick="onClick"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/storeOptionFourLeftImageView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.1"
                         />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/storeOptionFourTextView"
                        android:layout_width="0dp"
                        android:layout_weight="0.8"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/montserrat"
                        android:clickable="false"
                        android:padding="@dimen/_10sdp"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_13sdp"
                        tools:text="@string/refer_earn" />

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/storeOptionFourRightImageView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.1"
                        android:visibility="gone"
                        android:src="@drawable/ic_half_arrow_forward" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:background="@color/light_grey_background" />

                <LinearLayout
                    android:id="@+id/storeOptionFiveLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:selectableItemBackground"
                    android:onClick="onClick"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/storeOptionFiveLeftImageView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.1"
                         />

                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/storeOptionFiveTextView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.8"
                        android:clickable="false"
                        android:fontFamily="@font/montserrat"
                        android:padding="@dimen/_10sdp"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_13sdp"
                        tools:text="@string/refer_earn" />

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/storeOptionFiveRightImageView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.1"
                        android:visibility="gone"
                        android:src="@drawable/ic_half_arrow_forward" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_30sdp"
                    android:background="@color/white" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/dark_blackish"
                android:paddingStart="@dimen/_25sdp"
                android:paddingTop="@dimen/_10sdp"
                android:paddingEnd="@dimen/_25sdp"
                android:paddingBottom="@dimen/_10sdp"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
                app:layout_constraintVertical_bias="1.0">

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/autoDataBackupImageView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/ic_auto_data_backup" />

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/safeSecureImageView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/ic_auto_data_backup" />

            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>

</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>